<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Crossword.sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane fx:id="panel" prefHeight="649.0" prefWidth="899.0">
         <children>
            <Button fx:id="start" layoutX="119.0" layoutY="14.0" mnemonicParsing="false" onAction="#create" prefHeight="34.0" prefWidth="128.0" text="Stwórz krzyżówkę" />
            <ListView fx:id="listbooks" layoutX="454.0" layoutY="80.0" prefHeight="448.0" prefWidth="421.0" visible="false" />
            <Button fx:id="checkButton" layoutX="428.0" layoutY="14.0" mnemonicParsing="false" onAction="#check" prefHeight="34.0" prefWidth="128.0" text="Pokaż rozwiązanie" visible="false" />
            <TextField fx:id="SetXY" layoutX="45.0" layoutY="595.0" promptText="Podaj wymiar " />
         </children>
      </Pane>
   </children>
</GridPane>
