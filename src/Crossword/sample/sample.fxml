<GridPane alignment="center" hgap="10" prefHeight="704.0" prefWidth="1000.0" vgap="10" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Crossword.sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane fx:id="panel" prefHeight="700.0" prefWidth="1000.0">
         <children>
            <ListView fx:id="listbooks" layoutX="454.0" layoutY="80.0" prefHeight="448.0" prefWidth="421.0" visible="false" />
            <ToolBar layoutX="243.0" layoutY="17.0" prefHeight="54.0" prefWidth="344.0">
              <items>
                  <TextField fx:id="pathFileField" prefHeight="31.0" prefWidth="178.0" promptText="Wybierz ścieżke do pliku" />
                <Button fx:id="chooseFileButton" mnemonicParsing="false" onAction="#chooseFile" prefHeight="33.0" prefWidth="30.0" text="..." />
                  <Button fx:id="createButton" mnemonicParsing="false" onAction="#create" prefHeight="34.0" prefWidth="116.0" text="Generuj krzyżówkę" />
              </items>
            </ToolBar>
            <ToolBar layoutX="592.0" layoutY="17.0" prefHeight="54.0" prefWidth="403.0">
              <items>
                <Button fx:id="openButton" mnemonicParsing="false" onAction="#open" prefHeight="34.0" prefWidth="74.0" text="Otwórz" />
                  <Button fx:id="saveButton" mnemonicParsing="false" onAction="#save" prefHeight="34.0" prefWidth="74.0" text="Zapisz" />
                  <Button fx:id="pdfButton" mnemonicParsing="false" onAction="#pdfCreate" prefHeight="34.0" prefWidth="74.0" text="PDF" />
                  <Button fx:id="printButton" mnemonicParsing="false" onAction="#print" prefHeight="34.0" prefWidth="74.0" text="Drukuj" />
                  <Button fx:id="solveButton" mnemonicParsing="false" onAction="#solve" prefHeight="34.0" prefWidth="74.0" text="Rozwiąż" />
              </items>
            </ToolBar>
            <ToolBar layoutX="14.0" layoutY="17.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="54.0" prefWidth="217.0">
              <items>
                  <Label prefHeight="17.0" prefWidth="26.0" text="Wys" />
                  <Spinner fx:id="heightBordSpinner" prefHeight="25.0" prefWidth="55.0" />
                  <Label prefHeight="17.0" prefWidth="37.0" text="   Szer" />
                  <Spinner fx:id="widthBordSpinner" initialValue="15" prefHeight="25.0" prefWidth="51.0" />
              </items>
            </ToolBar>
            <Pane fx:id="panelBoard" layoutX="31.0" layoutY="120.0" prefHeight="460.0" prefWidth="421.0" />
         </children>
      </Pane>
   </children>
